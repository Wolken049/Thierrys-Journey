Numb = [5, 4, 7, 8, 6, 0, 9 ,1 , 2, 6, 2, 5, 6, 1]

item = input("Please enter item to be found: ")

count = 0

try:
    for i in range(len(Numb)):
        if Numb[i] == int(item):
            count += 1  
            continue
        
    print("Count:", count)
except Exception:
    print("Type a valid number")